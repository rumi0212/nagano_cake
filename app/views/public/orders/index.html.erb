<div class="container">
  <p>注文履歴</p>
  <table>
    <thead>
      <tr>
        <th>注文日</th>
        <th>配送先</th>
        <th>注文商品</th>
        <th>支払金額</th>
        <th>ステータス</th>
        <th>注文詳細</th>
      </tr>
    </thead>
    <tbody>
      <% @orders.each do |order|  %>
      <tr>
        <td><%= order.created_at %></td>
        <td><%= order.address %></td>
        <td><%=%></td>
        <td><%= order.total_payment %></td>
        <td><%= order.order_status %></td>
        <td><%= link_to "表示する", order_path(order) %> </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>